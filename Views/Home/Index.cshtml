@model Task2.Models.MarathonMember
@{
    ViewData["Title"] = "Регистрация участников марафона";
}

<form asp-action="Registration" asp-controller="Home" asp-antiforgery="true">
    <div asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="form-group">
            <label asp-for="Name">enter Name</label>
            <input type="text" asp-for="Name" />
            <span asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label asp-for="Gender">enter Gender</label>
            <select asp-for="Gender">
                <option value="М">М</option>
                <option value="Ж">Ж</option>
            </select>
            <span asp-validation-for="Gender"></span>
        </div>
        <div class="form-group">
            <label asp-for="Age">enter Age</label>
            <input asp-for="Age" />
            <span asp-validation-for="Age"></span>
        </div>
        <div class="form-group">
            <label asp-for="Experience">enter Experience</label>
            <input asp-for="Experience" />
            <span asp-validation-for="Experience"></span>
        </div>
        <div class="form-group">
            <label asp-for="CityOfResidence">enter CityOfResidence</label>
            <input asp-for="CityOfResidence" />
            <span asp-validation-for="CityOfResidence"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="enter" class="btn btn-outline-dark" />
        </div>
    </div>
</form>

<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.5.1.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.17.0/jquery.validate.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.10/jquery.validate.unobtrusive.min.js"></script>